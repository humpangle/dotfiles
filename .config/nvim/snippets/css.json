{
  "bc": {
    "prefix": ["comment_block", "block_comment"],
    "body": ["/*", "$0", "*/"],
    "description": ["Block comment"]
  },
  "wordpress theme": {
    "prefix": [
      "theme_wordpress_snippet",
      "theme_snippet_wordpress",
      "wordpress_theme_snippet"
    ],
    "body": [
      "@charset \"UTF-8\";",
      "",
      "/*",
      "Theme Name: ${1:Theme Name}",
      "Theme URI: https://${2:theme_url}",
      "Author: ${3:Theme Author}",
      "Author URI: https://${4:theme_url}",
      "Description: ${5:Description1}.",
      "${6:Description 2}",
      "Version: ${7:0.0.0}",
      "Requires at least: 5.3",
      "Requires PHP: 5.6",
      "License: GNU General Public License v2 or later",
      "License URI: http://www.gnu.org/licenses/gpl-2.0.html",
      "Text Domain: ${8:my-theme-folder-name}",
      "Tags: ${9:tag-1}",
      "*/"
    ],
    "description": ["Theme Wordpress Snippet"]
  }
}
