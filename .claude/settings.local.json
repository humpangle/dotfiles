{
  "env": {
    "DISABLE_TELEMETRY": "true"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Agent",
      "Bash",
      "Bash($HOME/dotfiles/scripts/_ai claudecs)",
